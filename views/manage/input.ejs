<div id=win_login style="display:none">
    <div id="input_toolbar">
        <p id="win_title"><p>
        <div id="input_close"><span>关闭</span></div>
    </div>
    <div id='div_form'>
        <form method="post" action='/manage_input'>
            <input type="hidden" value={{formtitle}} name="class">
            <div class="form-group">
                <label for="exampleInputEmail1" class="form_label">标题</label>
                <input type="text" class="form-control" id="exampleInputEmail1" placeholder="Title" name="title">
            </div>
            <div class="form-group">
                <label for="exampleInputPassword1" class="form_label">内容</label>

                <textarea class="form-control" rows="12" name="content"></textarea>
            </div>
            <div class="form-group">
                <label for="exampleInputFile" class="form_label">选取图片</label><br>
            </div>
            <div class="form-group">

                <input type="file" id="exampleInputFile" >
            </div>
            <button type="submit" class="btn btn-default" id="input_btn">提交</button>
        </form>
    </div>
</div>